@startuml
participant "Llamador" as C
participant "searchAndProcess" as S
participant "Array t" as T
participant "A" as PA
participant "B" as PB

C -> S: searchAndProcess(t, k)
S -> S: i = 0
loop mientras i < t.length && t[i] != k
    S -> T: leer t[i]
    T -> S: valor
    S -> S: i++
end
alt i < t.length && t[i] == k
    S -> PA: A()
    PA -> S: retorno
else i >= t.length
    S -> PB: B()
    PB -> S: retorno
end
S -> C: retorno
@enduml